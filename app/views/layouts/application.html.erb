<!DOCTYPE html>
<html>
<head>
<!-- FOR MAPS GOOGLE -->
  <script src="//maps.google.com/maps/api/js?key=AIzaSyDtJ-zJB0tDxrMvxmTCiGgb1UTo_6wFHyE"></script>
  <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
  <script src="//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js" type="text/javascript"></script>
  <!-- END MAPS GOOGLE -->

  <link href="https://fonts.googleapis.com/css?family=Aref+Ruqaa" rel="stylesheet">
  <!-- FOR NAVBAR -->
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>

    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">

    <!-- Latest compiled and minified JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/semantic-ui/2.2.6/semantic.min.css">
    <script src="https://cdn.jsdelivr.net/semantic-ui/2.2.6/semantic.min.js"></script>

  <!-- END NAVBAR -->
  <title>ConsumableApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <!-- NAVBAR -->
  <!-- NAVIGATION ELEMENTS -->
  <nav class="navbar navbar-default">
    <div class="container">
      <!-- Navbar Header and Branding -->
      <div class="navbar-header">
        <!-- Navabar Collapse Toggle -->
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="#">Welcome
        <% if !current_user.nil? %>
          <%= current_user.email %>
        <% end %>
        to Consumable</a>
      </div>
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav">
          <% if user_signed_in? %>
            <li><a href="/profiles/<%= current_user.profile.id %>">My Profile</a></li>
          <% end %>
          <li><a href="/profiles">Profile</a></li>
          <% if user_signed_in? %>
          <li><a href="#">My Wishlists</a></li>
          <li><a href="/reviews" id="review">Reviews</a></li>
          <li><a href="/search/search_by" id="search_by">Search By</a></li>
          <% end %>

          <!-- Dropdown menu -->
          <!-- <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Search By<span class="caret"></span></a>
            <ul class="dropdown-menu">
              <li><a href="#">Cuisine</a></li>
              <li><a href="#">Rating</a></li>
              <li><a href="#">Price</a></li> -->
              <!-- <li class="divider"></li> -->
              <!-- <li><a href="#">Location</a></li> -->
              <!-- <li class="divider"></li> -->
              <!-- <li><a href="#">One more separated link</a></li> -->
            <!-- </ul>
          </li> -->
            <ul class="nav navbar-nav navbar-right">
              <li>
                <div class="site-header-actions">
                  <!-- <input type="text" name="login_user_name" placeholder="User Name">
                  <input type="password" name="login_user_password" placeholder="Password"> -->
                  <% if !user_signed_in? %>
                    <a class="btn btn-success site-header-actions-btn mr-1" id="nav_log_in" href="/users/sign_in" data-ga-click="(Logged out) Header, clicked Sign up, text:sign-up"><span class="logintext">Log In</span></a>

                    <a class="btn btn-link" id="sign_up"
                    href="/users/sign_up" data-ga-click="(Logged out) Header, clicked Sign in, text:sign-in"><span class="signuptext">Sign Up</span></a>
                  <% end %>
                  <% if user_signed_in? %>
                     <%= link_to 'Logout', destroy_user_session_path, :method => :delete %>
                  <% end %>
                </div>
              </li>
            </ul>
        </ul>

        <!-- Dropdown menu fixed to the right -->
      </div> <!-- END of Collapse -->

      <!-- Everything else contained -->
      <%= yield %>
    </div> <!-- END Container -->
  </nav> <!-- END NAVBAR -->




</body>
</html>
